<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.modeldev.restauranttechnologynetwork.com/models/v3"
    version="3.0.0"
    xmlns:ms-0200="http://www.modeldev.restauranttechnologynetwork.com/models/v2"
    xmlns:ms-0300="http://www.modeldev.restauranttechnologynetwork.com/models/v3"
    xmlns:ota2="http://www.opentravel.org/OTM/Common/v0"
    xmlns:otm="http://www.OpenTravel.org/ns/OTA2/AppInfo_v01_00" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    
    <xsd:annotation>
        <xsd:appinfo>
            <otm:Library>
                <otm:LibraryName>RTN_Common</otm:LibraryName>
                <otm:LibraryVersion>3.0.0</otm:LibraryVersion>
                <otm:LibraryStatus>FINAL</otm:LibraryStatus>
                <otm:SourceFile>/RTN_Common_3_0_0.otm</otm:SourceFile>
                <otm:CompilerVersion>4.0-SNAPSHOT</otm:CompilerVersion>
                <otm:CompileDate>2022-09-21T10:17:56.403-04:00</otm:CompileDate>
            </otm:Library>
        </xsd:appinfo>
    </xsd:annotation>
    
    <xsd:import namespace="http://www.opentravel.org/OTM/Common/v0" schemaLocation="built-ins/OTM_BuiltIns.xsd"/>
    <xsd:import
        namespace="http://www.modeldev.restauranttechnologynetwork.com/models/v2" schemaLocation="ms-0200_imports.xsd"/>
    <xsd:import
        namespace="http://www.OpenTravel.org/ns/OTA2/AppInfo_v01_00" schemaLocation="built-ins/OTA2_AppInfo_v1.0.0.xsd"/>
    <xsd:include schemaLocation="RTN_Codelist_3_0_0_Trim.xsd"/>
    
    <xsd:element abstract="true" name="TimeSpanSubGrp" type="ms-0300:TimeSpan">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies a time span by intdicating the start and end dates/times.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">TimeSpan</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="TimeSpan"
        substitutionGroup="ms-0300:TimeSpanSubGrp" type="ms-0300:TimeSpan"/>
    <xsd:element name="TimeSpanSummary" type="ms-0300:TimeSpan"/>
    <xsd:element name="TimeSpanDetail"
        substitutionGroup="ms-0300:TimeSpanSubGrp" type="ms-0300:TimeSpan_Detail"/>
    
    <xsd:element abstract="true" name="EffectivitySubGrp" type="ms-0300:TimeSpan">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies a time span by intdicating the start and end dates/times.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">TimeSpan</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="Effectivity"
        substitutionGroup="ms-0300:EffectivitySubGrp" type="ms-0300:TimeSpan"/>
    <xsd:element name="EffectivitySummary" type="ms-0300:TimeSpan"/>
    <xsd:element name="EffectivityDetail"
        substitutionGroup="ms-0300:EffectivitySubGrp" type="ms-0300:TimeSpan_Detail"/>
    
    <xsd:complexType name="TimeSpan">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">TimeSpan</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="7" minOccurs="0" name="DaysOfWeek" type="ms-0200:DaysOfWeek_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the days of week.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="startDate" type="xsd:dateTime" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Specifies the start date/time.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="Example">2022-07-05T16:27:29+00:00</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="endDate" type="xsd:dateTime" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Specifies the end date/time.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="Example">2022-07-05T16:35:42+00:00</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="TimeSpan_Detail">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">TimeSpan</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ms-0300:TimeSpan">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="Name" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Marketing name associated with the availability of an item (e.g. Happy Hour).</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="4" minOccurs="0"
                        name="WeekOfMonth" type="xsd:positiveInteger">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies the specific week of the month that are applicable.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="12" minOccurs="0"
                        name="MonthOfYear" type="xsd:positiveInteger">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies the specific month of the year that is applicable.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Detail"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:simpleType name="GUID">
        <xsd:annotation>
            <xsd:documentation source="Description">A globally unique ID for an object.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">GUID</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="128"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:simpleType name="AlphaLength3">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">AlphaLength3</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z]{3}"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:element abstract="true" name="LocationSubGrp" type="ms-0300:Location_ID">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies details for a store or location.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Location</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="LocationID"
        substitutionGroup="ms-0300:LocationSubGrp" type="ms-0300:Location_ID"/>
    <xsd:element name="LocationIdentifier" type="ms-0300:Location_ID"/>
    <xsd:element name="Location"
        substitutionGroup="ms-0300:LocationSubGrp" type="ms-0300:Location"/>
    <xsd:element name="LocationSummary" type="ms-0300:Location"/>
    <xsd:element name="LocationDetail"
        substitutionGroup="ms-0300:LocationSubGrp" type="ms-0300:Location_Detail"/>
    
    <xsd:complexType name="Location_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Location</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="LocationID" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">A unique global identifier for the location.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="Location">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Location</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ms-0300:Location_ID">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="URI" type="xsd:anyURI">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The URI for the location.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ms-0300:Address">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies the physical location of the entity.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ms-0300:Position">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Used to specify the geographic coordinates of a location.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="LocationStatus" type="ms-0200:Status_Enum">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifes whether a location is open, temporarily closed or permanently closed.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:OperationSchedule">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies the hours the location is open for business.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Description" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation source="Description">A description of the location (e.g. store , ghost kitchen).</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:ChannelEffectivity">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies the the availability (days, and hours) as well as the channels (e.g. Delivery, Curbside, Take-out). for which the availability applies.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ms-0300:Phone">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Phone number associated with the location.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0"
                        name="PreferredStoreDescription" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Describes the context of the preferred store, such as for work, or for multibrand conglomerates, or aggregators.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ms-0300:Privacy">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Provides information as to the person&amp;apos;s preference for sharing information.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:CommunicationMethod">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Provides details for a specific form of communication.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0"
                        name="PreferredOrderChannel" type="ms-0200:OrderChannel_Enum">
                        <xsd:annotation>
                            <xsd:documentation source="Description">A favorite channel for placing orders for this store.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ms-0300:TermsAndConditions">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Code of conduct for the guest upon entering a restaurant (masks, no pics, ect.)</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
                </xsd:sequence>
                <xsd:attribute name="preferredStoreIndicatorInd"
                    type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, this store is a favorite.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="Location_Detail">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Location</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ms-0300:Location">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="OpenDate" type="xsd:dateTime">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies the date the restaurant is to open.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="ClosedDate" type="xsd:dateTime">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies the date a location is to close.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Detail"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="Address" type="ms-0300:Address"/>
    <xsd:element name="DeliveryLocation" type="ms-0300:Address"/>
    
    <xsd:complexType name="Address">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the physical location of an entity.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Address</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="StreetAddress" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The street number and street name.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="CityName" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The name of the city in which the entitiy is located.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="PostalCode" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the zip code for the entity.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Country" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the country in which the entity is located.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="CountryCode" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">ISO 3166 code for a country.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="StateOrTerritory" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the state, territory or province in which the entity is located,</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="AddressType" type="ms-0200:AddressType_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the type of address (e.g. home, business).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="OperationSchedule" type="ms-0300:OperationSchedule"/>
    
    <xsd:complexType name="OperationSchedule">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the days and times of operation.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">OperationSchedule</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:TimeSpanSummary">
                <xsd:annotation>
                    <xsd:documentation source="Description">The days and times a location is open for business.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Position" type="ms-0300:Position"/>
    
    <xsd:complexType name="Position">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the geographic position of the entity.  This position element uses the ISO 6709 standard.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Position</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Latitude" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The measure of the angular distance on the meridean north or south of the equator.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Longitude" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The measure of the angular distance on the meridian east or west of the prime meridian.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="ChannelEffectivity" type="ms-0300:ChannelEffectivity"/>
    
    <xsd:complexType name="ChannelEffectivity">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies availability for specific channels (e.g. curbside, take-out, delivery).</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">ChannelEffectivity</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0"
                name="Channel" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the channel for which the availabilty applies (e.g Curbside, Delivery, Take-out).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ms-0300:EffectivitySubGrp"/>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Phone" type="ms-0300:Phone"/>
    
    <xsd:complexType name="Phone">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies telephone information.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Phone</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" name="PhoneNumber" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The telephone number.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Type" type="ms-0200:PhoneType_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">The phone number type (e.g. business, mobile).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0"
                name="Capabilities" type="ms-0200:PhoneCapabilities_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the functionalities of the phone.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="primaryInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">When true, this is the main phone number for the person.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="PersonName" type="ms-0300:PersonName"/>
    
    <xsd:complexType name="PersonName">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the name of a person (e.g. customer, employee).</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">PersonName</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="First" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The first name or given name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Last" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The surname of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="Middle" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The middle name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Prefix" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Salutation for the person (e.g. Mr., Mrs., Ms., Dr., etc.)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Suffix" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">A suffix related to the name (e.g. Jr., Sr., III, Esq., etc.).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="NameType" type="ms-0200:NameType_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the type of name for the person (e.g. former, nickname, alternate, alias, etc.).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Title" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies degrees or honors of the person (e.g. M.D., Ph.D., etc.).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="LastNamePrefix" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the last name prefix (e.g. &amp;quot;Van der&amp;quot;, &amp;quot;von&amp;quot;, &amp;quot;de&amp;quot;, etc.).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="language" type="xsd:language" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The language in which the name is represented.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="Example">en-us</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="primaryInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">When true, this is the default or primary name for the person.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element abstract="true" name="PersonSubGrp" type="ms-0300:Person">
        <xsd:annotation>
            <xsd:documentation source="Description">Details about a person.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Person</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="Person" substitutionGroup="ms-0300:PersonSubGrp" type="ms-0300:Person"/>
    <xsd:element name="PersonSummary" type="ms-0300:Person"/>
    <xsd:element name="PersonDetail"
        substitutionGroup="ms-0300:PersonSubGrp" type="ms-0300:Person_Detail"/>
    
    <xsd:element abstract="true" name="ContactSubGrp" type="ms-0300:Person">
        <xsd:annotation>
            <xsd:documentation source="Description">Details about a person.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Person</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="Contact"
        substitutionGroup="ms-0300:ContactSubGrp" type="ms-0300:Person"/>
    <xsd:element name="ContactSummary" type="ms-0300:Person"/>
    <xsd:element name="ContactDetail"
        substitutionGroup="ms-0300:ContactSubGrp" type="ms-0300:Person_Detail"/>
    
    <xsd:complexType name="Person">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Person</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:PersonName">
                <xsd:annotation>
                    <xsd:documentation source="Description">The name of the person.  This may include given name, surname, etc..</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:Email">
                <xsd:annotation>
                    <xsd:documentation source="Description">Email address details for the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:Phone">
                <xsd:annotation>
                    <xsd:documentation source="Description">Phone details associated with this person.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:Address">
                <xsd:annotation>
                    <xsd:documentation source="Description">Addresses associated with this person.</xsd:documentation>
