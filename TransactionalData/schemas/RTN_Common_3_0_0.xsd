    <xsd:complexType name="CurrencyCode">
        <xsd:annotation>
            <xsd:documentation source="Description">An ISO 4217 alpha character code used to specify the monetary unit.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">CurrencyCode</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ms-0300:AlphaLength3">
                <xsd:attribute name="decimalPlaces" type="xsd:string" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">An ISO 4201 &amp;quot;minor unit&amp;quot; typically used when an amount is sent without the decimal places to specify where the decimal should be placed (e.g. 2500 USD with 2 minor units would represent $25.00)</xsd:documentation>
                        <xsd:appinfo>
                            <otm:Example context="ms">2</otm:Example>
                        </xsd:appinfo>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:element name="PackingList" type="ms-0300:PackingList"/>
    
    <xsd:complexType name="PackingList">
        <xsd:annotation>
            <xsd:documentation source="Description">A items that should be packed for this menu item (e.g. utensils, napkins).</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">PackingList</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="ItemName" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The name of the item to be packed.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Quantity" type="xsd:integer">
                <xsd:annotation>
                    <xsd:documentation source="Description">The number of the packing item to be included.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Email" type="ms-0300:Email"/>
    
    <xsd:complexType name="Email">
        <xsd:annotation>
            <xsd:documentation source="Description">Email address information.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Email</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" name="EmailAddress" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">An email address.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="EmailType" type="ms-0200:AddressType_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the type of email (e.g. Business, Personal).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="primaryInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">When true, this is the primary email address for the person.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element abstract="true" name="ProfileSubGrp" type="ms-0300:Profile_ID">
        <xsd:annotation>
            <xsd:documentation source="Description">Details related to a an individual or company.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Profile</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ProfileID"
        substitutionGroup="ms-0300:ProfileSubGrp" type="ms-0300:Profile_ID"/>
    <xsd:element name="ProfileIdentifier" type="ms-0300:Profile_ID"/>
    <xsd:element name="Profile"
        substitutionGroup="ms-0300:ProfileSubGrp" type="ms-0300:Profile"/>
    <xsd:element name="ProfileSummary" type="ms-0300:Profile"/>
    
    <xsd:complexType name="Profile_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Profile</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="ProfileID" type="ms-0300:GUID">
                <xsd:annotation>
                    <xsd:documentation source="Description">A globally unique identifier for a customer (person, entity, group).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="UserID" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">An customer facing identifier for the profile.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="Profile">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Profile</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ms-0300:Profile_ID">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="Status" type="ms-0200:ProfileStatus_Enum">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies the current status of the profile (e.g. Active, Inactive, Marked for deletion, etc.).</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:ProfileStatusHistory">
                        <xsd:annotation>
                            <xsd:documentation source="Description">A collection of status changes for a profile along with associated data.  Even the current status will create a ProfileStatusHistory Record which can be used to calculate when a status began and potentially ended.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:AssociatedProfileSubGrp">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Provides information regarding another profile and the relationship to the profile.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="1" name="LastUsedDateTime" type="xsd:dateTime">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies the last time that the profile was utilized (e.g. updated, usage, etc.).</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ms-0300:ProfileEntity">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Profile information for a person or company.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ms-0300:BusinessEntity">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The organization doing business as a portfolio holding company, brand or order fulfillment location.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="SocialMedia" type="ms-0300:SocialMedia"/>
    
    <xsd:complexType name="SocialMedia">
        <xsd:annotation>
            <xsd:documentation source="Description">SocialMedia</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">SocialMedia</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="URL" type="xsd:anyURI">
                <xsd:annotation>
                    <xsd:documentation source="Description">The URL for the social media platform.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" name="Platform" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The name of the social media platform.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" name="Handle" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The person&amp;apos;s handle for the social media platform.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="1" name="UserName" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The user name for the person on the social media platform.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="CommunicationMethod" type="ms-0300:CommunicationMethod"/>
    
    <xsd:complexType name="CommunicationMethod">
        <xsd:annotation>
            <xsd:documentation source="Description">Provides details for a specific form of communication.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">CommunicationMethod</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="CommunicationMethod" type="ms-0200:CommunicationMethod_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">A type of communication used by the individual.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="PreferredLanguage" type="xsd:language">
                <xsd:annotation>
                    <xsd:documentation source="Description">The  preferred language for communications.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="primaryInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">When true, this is the primary method for communication for this individual.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="ProfileStatusHistory" type="ms-0300:ProfileStatusHistory"/>
    
    <xsd:complexType name="ProfileStatusHistory">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">ProfileStatusHistory</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" name="Status" type="ms-0200:ProfileStatus_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the status of the profile at a previous date and time.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ChangeDate" type="xsd:dateTime">
                <xsd:annotation>
                    <xsd:documentation source="Description">The date and time that the status changed (ISO 8601), this is preferred data and should be sent if available.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ChangedBy" type="ms-0300:GUID">
                <xsd:annotation>
                    <xsd:documentation source="Description">An identifier for the system that made the status change on the profile.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="SystemName" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The name of the system that made the status change.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Comment" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Provides details around the nature or cause of the status change.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element abstract="true" name="AssociatedProfileSubGrp" type="ms-0300:AssociatedProfile">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">AssociatedProfile</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="AssociatedProfile"
        substitutionGroup="ms-0300:AssociatedProfileSubGrp" type="ms-0300:AssociatedProfile"/>
    <xsd:element name="AssociatedProfileSummary" type="ms-0300:AssociatedProfile"/>
    <xsd:element name="AssociatedProfileDetail"
        substitutionGroup="ms-0300:AssociatedProfileSubGrp" type="ms-0300:AssociatedProfile_Detail"/>
    
    <xsd:complexType name="AssociatedProfile">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">AssociatedProfile</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="ProfileID" type="ms-0300:GUID">
                <xsd:annotation>
                    <xsd:documentation source="Description">The globally unique identifier for the associated profile.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="UserID" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The customer facing ID for the profile.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0"
                name="RelationshipType" type="ms-0200:RelationshipType_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Defines the nature of the associated profile.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Description" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Description of the relationship between the associated profile and the profile on the record.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="AssociatedProfile_Detail">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">AssociatedProfile</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ms-0300:AssociatedProfile">
                <xsd:sequence>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Detail"/>
                </xsd:sequence>
                <xsd:attribute name="mergeInd" type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, this profile will be merged with the primary profile.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element abstract="true" name="ThirdPartySystemSubGrp" type="ms-0300:ThirdPartySystem_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">ThirdPartySystem</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ThirdPartySystemID"
        substitutionGroup="ms-0300:ThirdPartySystemSubGrp" type="ms-0300:ThirdPartySystem_ID"/>
    <xsd:element name="ThirdPartySystemIdentifier" type="ms-0300:ThirdPartySystem_ID"/>
    <xsd:element name="ThirdPartySystem"
        substitutionGroup="ms-0300:ThirdPartySystemSubGrp" type="ms-0300:ThirdPartySystem"/>
    <xsd:element name="ThirdPartySystemSummary" type="ms-0300:ThirdPartySystem"/>
    
    <xsd:complexType name="ThirdPartySystem_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">ThirdPartySystem</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" name="ThirdPartySystemID" type="ms-0300:GUID">
                <xsd:annotation>
                    <xsd:documentation source="Description">A globally unique identifier for a third party system.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="ThirdPartySystem">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">ThirdPartySystem</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ms-0300:ThirdPartySystem_ID">
                <xsd:sequence>
                    <xsd:element minOccurs="1" name="Type" type="ms-0200:ThirdPartySystemType_Enum">
                        <xsd:annotation>
                            <xsd:documentation source="Description">A classification for the type of third party system.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="SystemName" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The name of the third party system.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="CustomerID" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation source="Description">An ID for the customer within the third party system.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="ProgramID" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation source="Description">An identifier for the program (birthday club, new guest club, tier within a loyalty system, etc.)</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:simpleType name="YesNoInherit_Enum">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="EnumerationClosed">YesNoInherit_Enum</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Yes"/>
            <xsd:enumeration value="No"/>
            <xsd:enumeration value="Inherit"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:element name="TermsAndConditions" type="ms-0300:TermsAndConditions"/>
    
    <xsd:complexType name="TermsAndConditions">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">TermsAndConditions</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" name="IssuerID" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">An identifier for the issuer of the terms and conditions.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" name="IssuerDescription" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The name or description for the issuer.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" name="Description" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">A description of the terms and conditions.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" name="DateTime" type="xsd:dateTime">
                <xsd:annotation>
                    <xsd:documentation source="Description">The date and time that the customer reviewed the terms and conditions.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="acceptanceInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">When true, the customer as agreed to the terms and conditions.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="Verification" type="ms-0300:Verification"/>
    
    <xsd:complexType name="Verification">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies information for the person that has been previously verified.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Verification</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" name="VerificationCategory" type="ms-0200:VerificationCategory_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the type of verification that has been performed.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" name="VerificationType" type="ms-0200:VerificationType_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Identifies the type of document used for verification.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="VerifiedBy" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Identifies a person who verified the ID.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="VerificationSystem" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Identifies the verification system.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Description" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">A name or description for the verification system.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="VerifiedInd" type="xsd:boolean">
                <xsd:annotation>
                    <xsd:documentation source="Description">When true, this verification category has been verified.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Brand" type="ms-0300:Brand"/>
    
    <xsd:complexType name="Brand">
        <xsd:annotation>
            <xsd:documentation source="Description">A group of restaurants belonging to the same company.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Brand</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="BrandName" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The brand name for the group of restaurants (i.e. McDonalds, Panera, etc.).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="1" name="BrandID" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">An identifer for the brand.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="PreferredBrandDescription" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Describes the context of the preferred brand, such as for work, or for multibrand conglomerates, or aggregators.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:LocationSubGrp">
                <xsd:annotation>
                    <xsd:documentation source="Description">Provides details for a specific store.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ms-0300:Privacy">
                <xsd:annotation>
                    <xsd:documentation source="Description">Provides information as to the person&amp;apos;s preference for sharing information.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" ref="ms-0300:CommunicationMethod">
                <xsd:annotation>
                    <xsd:documentation source="Description">Provides details for a specific form of communication.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:TermsAndConditions">
                <xsd:annotation>
                    <xsd:documentation source="Description">Code of conduct for the guest upon entering a restaurant (masks, no pics, ect.)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0"
                name="PreferredOrderChannel" type="ms-0200:OrderChannel_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">A favorite channel for placing orders for this brand.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="BrandDescription" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Provides descriptive information about the brand.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="preferredBrandInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">When true, this is a favorite brand.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="Privacy" type="ms-0300:Privacy"/>
    
    <xsd:complexType name="Privacy">
        <xsd:annotation>
            <xsd:documentation source="Description">Allows for control of the sharing of data between parties.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Privacy</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="ShareMarketIndicator" type="ms-0300:YesNoInherit_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Permission for sharing information for marketing purposes.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="ShareSyncIndicator" type="ms-0300:YesNoInherit_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Permission for sharing data for synchonization of information held by other service providers.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element abstract="true" name="ProfileEntity" type="ms-0300:ProfileEntity">
        <xsd:annotation>
            <xsd:documentation source="Description">Profile information for a person or company.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ChoiceObject">ProfileEntity</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="ProfileEntityCompany"
        substitutionGroup="ms-0300:ProfileEntity" type="ms-0300:ProfileEntity_Company"/>
    <xsd:element name="ProfileEntityPerson"
        substitutionGroup="ms-0300:ProfileEntity" type="ms-0300:ProfileEntity_Person"/>
    
    <xsd:complexType abstract="true" name="ProfileEntity">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ChoiceObject">ProfileEntity</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Description" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Description related to the entitiy.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="ProfileEntity_Company">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ChoiceObject">ProfileEntity</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ms-0300:ProfileEntity">
                <xsd:sequence>
                    <xsd:element minOccurs="0" ref="ms-0200:CompanySubGrp">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Profile informaation for a business.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Choice"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="ProfileEntity_Person">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ChoiceObject">ProfileEntity</otm:OTA2Entity>
