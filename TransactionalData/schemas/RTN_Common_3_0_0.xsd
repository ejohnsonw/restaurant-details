<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified"
    elementFormDefault="qualified"
    targetNamespace="http://www.modeldev.restauranttechnologynetwork.com/models/v3"
    version="3.0.0"
    xmlns:ms-0200="http://www.modeldev.restauranttechnologynetwork.com/models/v2"
    xmlns:ms-0300="http://www.modeldev.restauranttechnologynetwork.com/models/v3"
    xmlns:ota2="http://www.opentravel.org/OTM/Common/v0"
    xmlns:otm="http://www.OpenTravel.org/ns/OTA2/AppInfo_v01_00" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    
    <xsd:annotation>
        <xsd:appinfo>
            <otm:Library>
                <otm:ProjectName>RTN_Transactions_FINAL.otp</otm:ProjectName>
                <otm:LibraryName>RTN_Common</otm:LibraryName>
                <otm:LibraryVersion>3.0.0</otm:LibraryVersion>
                <otm:LibraryStatus>FINAL</otm:LibraryStatus>
                <otm:SourceFile>/RTN_Common_3_0_0.otm</otm:SourceFile>
                <otm:CompilerVersion>4.0-SNAPSHOT</otm:CompilerVersion>
                <otm:CompileDate>2022-09-21T10:17:46.383-04:00</otm:CompileDate>
            </otm:Library>
        </xsd:appinfo>
    </xsd:annotation>
    
    <xsd:import namespace="http://www.opentravel.org/OTM/Common/v0" schemaLocation="built-ins/OTM_BuiltIns.xsd"/>
    <xsd:import
        namespace="http://www.modeldev.restauranttechnologynetwork.com/models/v2" schemaLocation="ms-0200_imports.xsd"/>
    <xsd:import
        namespace="http://www.modeldev.restauranttechnologynetwork.com/models/v2" schemaLocation="ms-0200_imports.xsd"/>
    <xsd:import
        namespace="http://www.OpenTravel.org/ns/OTA2/AppInfo_v01_00" schemaLocation="built-ins/OTA2_AppInfo_v1.0.0.xsd"/>
    <xsd:include schemaLocation="RTN_Codelist_3_0_0.xsd"/>
    
    <xsd:element abstract="true" name="TimeSpanSubGrp" type="ms-0300:TimeSpan">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies a time span by intdicating the start and end dates/times.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">TimeSpan</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="TimeSpan"
        substitutionGroup="ms-0300:TimeSpanSubGrp" type="ms-0300:TimeSpan"/>
    <xsd:element name="TimeSpanSummary" type="ms-0300:TimeSpan"/>
    <xsd:element name="TimeSpanDetail"
        substitutionGroup="ms-0300:TimeSpanSubGrp" type="ms-0300:TimeSpan_Detail"/>
    
    <xsd:element abstract="true" name="EffectivitySubGrp" type="ms-0300:TimeSpan">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies a time span by intdicating the start and end dates/times.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">TimeSpan</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="Effectivity"
        substitutionGroup="ms-0300:EffectivitySubGrp" type="ms-0300:TimeSpan"/>
    <xsd:element name="EffectivitySummary" type="ms-0300:TimeSpan"/>
    <xsd:element name="EffectivityDetail"
        substitutionGroup="ms-0300:EffectivitySubGrp" type="ms-0300:TimeSpan_Detail"/>
    
    <xsd:complexType name="TimeSpan">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">TimeSpan</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="7" minOccurs="0" name="DaysOfWeek" type="ms-0200:DaysOfWeek_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the days of week.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="startDate" type="xsd:dateTime" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Specifies the start date/time.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="Example">2022-07-05T16:27:29+00:00</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="endDate" type="xsd:dateTime" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Specifies the end date/time.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="Example">2022-07-05T16:35:42+00:00</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="TimeSpan_Detail">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">TimeSpan</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ms-0300:TimeSpan">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="Name" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Marketing name associated with the availability of an item (e.g. Happy Hour).</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="4" minOccurs="0"
                        name="WeekOfMonth" type="xsd:positiveInteger">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies the specific week of the month that are applicable.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="12" minOccurs="0"
                        name="MonthOfYear" type="xsd:positiveInteger">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies the specific month of the year that is applicable.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Detail"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:simpleType name="GUID">
        <xsd:annotation>
            <xsd:documentation source="Description">A globally unique ID for an object.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">GUID</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="128"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:complexType name="SKU">
        <xsd:annotation>
            <xsd:documentation source="Description">The Stock Keeping Unit.  A code that identifies a product.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ValueWithAttributes">SKU</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="ota2:Empty">
                <xsd:attribute name="productName" type="xsd:string" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The name of the product for which the product code belongs.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
                <xsd:attribute name="productCode" type="xsd:string" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">The SKU code for the product.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    
    <xsd:element name="Pricing" type="ms-0300:Pricing"/>
    
    <xsd:complexType name="Pricing">
        <xsd:annotation>
            <xsd:documentation source="Description">Defines the price, tax and related fees for an item.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Pricing</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Currency" type="ms-0300:CurrencyCode">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the currency information for the amount.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:Surcharge">
                <xsd:annotation>
                    <xsd:documentation source="Description">A collection of surcharges associated with the item (e.g. bag charge).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:Tax">
                <xsd:annotation>
                    <xsd:documentation source="Description">A collection of taxes associated with the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="1" name="ItemPrice" type="xsd:decimal">
                <xsd:annotation>
                    <xsd:documentation source="Description">The cost of an item.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0"
                name="Context" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The order channel, daypart, effectivity for which this pricing item applies.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:Discount">
                <xsd:annotation>
                    <xsd:documentation source="Description">Describes a discount and to which items it applies.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="taxExemptInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">When true, this item is not taxable  When false, this item is taxable.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:simpleType name="AlphaLength3">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="Simple">AlphaLength3</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z]{3}"/>
        </xsd:restriction>
    </xsd:simpleType>
    
    <xsd:element name="Media" type="ms-0300:Media"/>
    
    <xsd:complexType name="Media">
        <xsd:annotation>
            <xsd:documentation source="Description">Provides a link to a media URI or Known Asseet Number.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Media</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="MediaURI" type="xsd:anyURI">
                <xsd:annotation>
                    <xsd:documentation source="Description">The URI for the media asset.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="KnownAssetNumber" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The Known Asset Number for the media asset.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="MediaCategory" type="ms-0300:MediaCategory"/>
    
    <xsd:complexType name="MediaCategory">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the category for the media asset contained within the object.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">MediaCategory</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Name" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The name of the category for this specific media asset.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ms-0300:Media">
                <xsd:annotation>
                    <xsd:documentation source="Description">A media asset for the specified category.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element abstract="true" name="Availability" type="ms-0300:Availability">
        <xsd:annotation>
            <xsd:documentation source="Description">Allows either the use of the effectivity object or the day part object to be used to define the availability.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ChoiceObject">Availability</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="AvailabilityEffectivity"
        substitutionGroup="ms-0300:Availability" type="ms-0300:Availability_Effectivity"/>
    <xsd:element name="AvailabilityDayPart"
        substitutionGroup="ms-0300:Availability" type="ms-0300:Availability_DayPart"/>
    
    <xsd:complexType abstract="true" name="Availability">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ChoiceObject">Availability</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="Availability_Effectivity">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ChoiceObject">Availability</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ms-0300:Availability">
                <xsd:sequence>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0200:EffectivitySubGrp">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Describes the effective dates/times that the item being described is available.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Choice"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="Availability_DayPart">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="ChoiceObject">Availability</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ms-0300:Availability">
                <xsd:sequence>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0200:DayPart">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies a portion of the day.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Choice"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="DayPart" type="ms-0300:DayPart"/>
    
    <xsd:complexType name="DayPart">
        <xsd:annotation>
            <xsd:documentation source="Description">Defines a specific part of the day (e.g. the days/hours that breakfast is available).  This object makes use of the summary facet on the TimeSpan object.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">DayPart</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Name" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the name of the day part (e.g. Breakfast, Lunch, Brunch, Dinner).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:TimeSpanSummary">
                <xsd:annotation>
                    <xsd:documentation source="Description">The start and end date and time for the day part.  May also include the day of week.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="uRI" type="xsd:anyURI" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">A URI to day part details.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="Tax" type="ms-0300:Tax"/>
    
    <xsd:complexType name="Tax">
        <xsd:annotation>
            <xsd:documentation source="Description">Detailed tax information.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Tax</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Description" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">A textual description of the tax.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="amount" type="xsd:decimal" use="required">
            <xsd:annotation>
                <xsd:documentation source="Description">The system calculated tax amount.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="ms">.53</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="taxIncludedInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">When true, tax is included in the price amount.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="Surcharge" type="ms-0300:Surcharge"/>
    
    <xsd:complexType name="Surcharge">
        <xsd:annotation>
            <xsd:documentation source="Description">Detailed surcharge information (e.g. a charge added to a price).</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Surcharge</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Description" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">A textual description of the surcharge.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="amount" type="xsd:decimal" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The cost of the surcharge.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="ms">3.00</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="surchargeIncludedInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">When true, the surcharge is included in the item price.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="Discount" type="ms-0300:Discount"/>
    
    <xsd:complexType name="Discount">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the details of a discount such as the amout or percent of the discount and to which items the discount applies.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Discount</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0"
                name="Application" type="ms-0300:GUID">
                <xsd:annotation>
                    <xsd:documentation source="Description">A discount that applies to the item.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Description" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">A description of the discount.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="valueType" type="ms-0200:ValueType_Enum" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">Specifies if the discount is expressed as an amount or a percent.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="ms">Amount</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="value" type="xsd:decimal" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The discount amount or percentage.  The value type defines whether the value is an amount or a percent.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="ms">3.00</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="beforeTaxInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">When true, the discount is applied before tax.  When false, the discount applies after tax.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element abstract="true" name="LocationSubGrp" type="ms-0300:Location_ID">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies details for a store or location.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Location</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
    </xsd:element>
    <xsd:element name="LocationID"
        substitutionGroup="ms-0300:LocationSubGrp" type="ms-0300:Location_ID"/>
    <xsd:element name="LocationIdentifier" type="ms-0300:Location_ID"/>
    <xsd:element name="Location"
        substitutionGroup="ms-0300:LocationSubGrp" type="ms-0300:Location"/>
    <xsd:element name="LocationSummary" type="ms-0300:Location"/>
    <xsd:element name="LocationDetail"
        substitutionGroup="ms-0300:LocationSubGrp" type="ms-0300:Location_Detail"/>
    
    <xsd:complexType name="Location_ID">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Location</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="LocationID" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">A unique global identifier for the location.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="Location">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Location</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ms-0300:Location_ID">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="URI" type="xsd:anyURI">
                        <xsd:annotation>
                            <xsd:documentation source="Description">The URI for the location.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ms-0300:Address">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies the physical location of the entity.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ms-0300:Position">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Used to specify the geographic coordinates of a location.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="LocationStatus" type="ms-0200:Status_Enum">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifes whether a location is open, temporarily closed or permanently closed.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:OperationSchedule">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies the hours the location is open for business.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="Description" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation source="Description">A description of the location (e.g. store , ghost kitchen).</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:ChannelEffectivity">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies the the availability (days, and hours) as well as the channels (e.g. Delivery, Curbside, Take-out). for which the availability applies.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ms-0300:Phone">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Phone number associated with the location.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0"
                        name="PreferredStoreDescription" type="xsd:string">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Describes the context of the preferred store, such as for work, or for multibrand conglomerates, or aggregators.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ms-0300:Privacy">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Provides information as to the person&amp;apos;s preference for sharing information.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:CommunicationMethod">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Provides details for a specific form of communication.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0"
                        name="PreferredOrderChannel" type="ms-0200:OrderChannel_Enum">
                        <xsd:annotation>
                            <xsd:documentation source="Description">A favorite channel for placing orders for this store.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ms-0300:TermsAndConditions">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Code of conduct for the guest upon entering a restaurant (masks, no pics, ect.)</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
                </xsd:sequence>
                <xsd:attribute name="preferredStoreIndicatorInd"
                    type="xsd:boolean" use="optional">
                    <xsd:annotation>
                        <xsd:documentation source="Description">When true, this store is a favorite.</xsd:documentation>
                    </xsd:annotation>
                </xsd:attribute>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:complexType name="Location_Detail">
        <xsd:annotation>
            <xsd:appinfo>
                <otm:OTA2Entity type="BusinessObject">Location</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:complexContent>
            <xsd:extension base="ms-0300:Location">
                <xsd:sequence>
                    <xsd:element minOccurs="0" name="OpenDate" type="xsd:dateTime">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies the date the restaurant is to open.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" name="ClosedDate" type="xsd:dateTime">
                        <xsd:annotation>
                            <xsd:documentation source="Description">Specifies the date a location is to close.</xsd:documentation>
                        </xsd:annotation>
                    </xsd:element>
                    <xsd:element minOccurs="0" ref="ota2:ExtensionPoint_Detail"/>
                </xsd:sequence>
            </xsd:extension>
        </xsd:complexContent>
    </xsd:complexType>
    
    <xsd:element name="Address" type="ms-0300:Address"/>
    <xsd:element name="DeliveryLocation" type="ms-0300:Address"/>
    
    <xsd:complexType name="Address">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the physical location of an entity.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Address</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="StreetAddress" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The street number and street name.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="CityName" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The name of the city in which the entitiy is located.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="PostalCode" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the zip code for the entity.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Country" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the country in which the entity is located.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="CountryCode" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">ISO 3166 code for a country.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="StateOrTerritory" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the state, territory or province in which the entity is located,</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="AddressType" type="ms-0200:AddressType_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the type of address (e.g. home, business).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="OperationSchedule" type="ms-0300:OperationSchedule"/>
    
    <xsd:complexType name="OperationSchedule">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the days and times of operation.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">OperationSchedule</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ms-0300:TimeSpanSummary">
                <xsd:annotation>
                    <xsd:documentation source="Description">The days and times a location is open for business.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Position" type="ms-0300:Position"/>
    
    <xsd:complexType name="Position">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the geographic position of the entity.  This position element uses the ISO 6709 standard.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Position</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="Latitude" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The measure of the angular distance on the meridean north or south of the equator.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Longitude" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The measure of the angular distance on the meridian east or west of the prime meridian.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="ChannelEffectivity" type="ms-0300:ChannelEffectivity"/>
    
    <xsd:complexType name="ChannelEffectivity">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies availability for specific channels (e.g. curbside, take-out, delivery).</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">ChannelEffectivity</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="0"
                name="Channel" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the channel for which the availabilty applies (e.g Curbside, Delivery, Take-out).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ms-0300:EffectivitySubGrp"/>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="Phone" type="ms-0300:Phone"/>
    
    <xsd:complexType name="Phone">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies telephone information.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Phone</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="1" name="PhoneNumber" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The telephone number.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Type" type="ms-0200:PhoneType_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">The phone number type (e.g. business, mobile).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="unbounded" minOccurs="0"
                name="Capabilities" type="ms-0200:PhoneCapabilities_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the functionalities of the phone.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="primaryInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">When true, this is the main phone number for the person.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element name="Promotion" type="ms-0300:Promotion"/>
    
    <xsd:complexType name="Promotion">
        <xsd:annotation>
            <xsd:documentation source="Description">Provides information regarding a promotion or charity event.</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">Promotion</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="PromotionCode" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">A code that the guest provides to invoke a discount on the menu product (e.g. promotion, charity event).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Description" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Provides a description of the promotion or charity event identified in the promotion code.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
    </xsd:complexType>
    
    <xsd:element name="PersonName" type="ms-0300:PersonName"/>
    <xsd:element name="CustomerName" type="ms-0300:PersonName"/>
    
    <xsd:complexType name="PersonName">
        <xsd:annotation>
            <xsd:documentation source="Description">Specifies the name of a person (e.g. customer, employee).</xsd:documentation>
            <xsd:appinfo>
                <otm:OTA2Entity type="CoreObject">PersonName</otm:OTA2Entity>
            </xsd:appinfo>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element minOccurs="0" name="First" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The first name or given name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Last" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The surname of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element maxOccurs="1" minOccurs="0" name="Middle" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">The middle name of the person.</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Prefix" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Salutation for the person (e.g. Mr., Mrs., Ms., Dr., etc.)</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Suffix" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">A suffix related to the name (e.g. Jr., Sr., III, Esq., etc.).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="NameType" type="ms-0200:NameType_Enum">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the type of name for the person (e.g. former, nickname, alternate, alias, etc.).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Title" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies degrees or honors of the person (e.g. M.D., Ph.D., etc.).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="LastNamePrefix" type="xsd:string">
                <xsd:annotation>
                    <xsd:documentation source="Description">Specifies the last name prefix (e.g. &amp;quot;Van der&amp;quot;, &amp;quot;von&amp;quot;, &amp;quot;de&amp;quot;, etc.).</xsd:documentation>
                </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="language" type="xsd:language" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The language in which the name is represented.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="Example">en-us</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="primaryInd" type="xsd:boolean" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">When true, this is the default or primary name for the person.</xsd:documentation>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:element abstract="true" name="PersonSubGrp" type="ms-0300:Person">
