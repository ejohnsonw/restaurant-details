            </xsd:element>
            <xsd:element minOccurs="0" ref="ota2:ExtensionPoint"/>
        </xsd:sequence>
        <xsd:attribute name="birthday" type="xsd:date" use="optional">
            <xsd:annotation>
                <xsd:documentation source="Description">The date of birth.</xsd:documentation>
                <xsd:appinfo>
                    <otm:Example context="Example">2000-11-30</otm:Example>
                </xsd:appinfo>
            </xsd:annotation>
        </xsd:attribute>
    </xsd:complexType>
    
    <xsd:complexType name="Person_Detail">
